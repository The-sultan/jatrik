    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />
    
	<uses-permission 
	    android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="uy.edu.fing.tsi2.jatrik.android.main.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="uy.edu.fing.tsi2.jatrik.android.main.HistorialPartidosActivity"
            android:label="@string/title_activity_historial_partidos">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="uy.edu.fing.tsi2.jatrik.android.main.MainActivity" />
        </activity>
    </application>

</manifest>
